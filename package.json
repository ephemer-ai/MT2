  "scripts": {
    "build": "npm run compress",
    "bundle": "scripts/bundle.sh",
    "cache": "scripts/sw.sh",
    "clean": "rm html/*.br html/*.gz data/*.br data/*.gz js/*.br js/*.gz css/*.br css/*.gz vendor/*.br vendor/*.gz workers/*.br workers/*.gz",
